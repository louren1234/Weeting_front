<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
xmlns:app="http://schemas.android.com/apk/res-auto"
xmlns:tools="http://schemas.android.com/tools"
android:layout_width="match_parent"
android:layout_height="match_parent"
tools:context=".MainActivity">

<ScrollView
    android:layout_width="match_parent"
    android:layout_height="match_parent">

<LinearLayout
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:orientation="vertical"
    tools:layout_editor_absoluteX="1dp"
    tools:layout_editor_absoluteY="152dp">

    <LinearLayout
        android:id="@+id/linearLayout2"
        android:layout_width="408dp"
        android:layout_height="115dp"
        android:gravity="bottom">

        <TextView
            android:id="@+id/mainpage"
            android:layout_width="270dp"
            android:layout_height="match_parent"
            android:layout_marginStart="20dp"
            android:gravity="center_vertical"
            android:text="@string/weeting"
            android:textColor="@color/mainColor"
            android:textSize="40sp"
            app:autoSizeMaxTextSize="40sp"
            app:autoSizeTextType="uniform" />

        <View
            android:layout_width="0dp"
            android:layout_height="0dp"
            android:layout_weight="1" />

        <ImageButton
            android:id="@+id/search"
            android:layout_width="31dp"
            android:layout_height="match_parent"
            android:layout_margin="20dp"
            android:background="@android:color/transparent"
            android:contentDescription="settings"
            android:gravity="center_vertical"
            android:scaleType="centerInside"
            app:srcCompat="@drawable/search"
            tools:ignore="HardcodedText" />

    </LinearLayout>

    <LinearLayout

        android:id="@+id/linearLayout3"
        android:layout_width="414dp"
        android:layout_height="34dp"
        android:gravity="center"
        android:orientation="horizontal">

        <ImageButton
            android:id="@+id/toHome"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center"
            android:layout_weight="1"
            android:background="@android:color/transparent"
            android:contentDescription="home"
            android:scaleType="centerInside"
            app:srcCompat="@drawable/home"
            tools:ignore="HardcodedText" />

        <ImageButton
            android:id="@+id/toList"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:background="@android:color/transparent"
            android:contentDescription="list"
            android:scaleType="centerInside"
            app:srcCompat="@drawable/list"
            tools:ignore="HardcodedText" />

        <ImageButton
            android:id="@+id/toMap"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:background="@android:color/transparent"
            android:contentDescription="map"
            android:scaleType="centerInside"
            app:srcCompat="@drawable/pin"
            tools:ignore="HardcodedText" />

        <ImageButton
            android:id="@+id/toCalender"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:background="@android:color/transparent"
            android:contentDescription="calender"
            android:scaleType="centerInside"
            app:srcCompat="@drawable/calendar"
            tools:ignore="HardcodedText" />

        <ImageButton
            android:id="@+id/toMypage"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:background="@android:color/transparent"
            android:contentDescription="mypage"
            android:scaleType="centerInside"
            app:srcCompat="@drawable/user"
            tools:ignore="HardcodedText" />

    </LinearLayout>

    <LinearLayout
        android:layout_width="409dp"
        android:layout_height="73dp"
        android:gravity="center_vertical"
        android:orientation="vertical"
        tools:layout_editor_absoluteX="-1dp"
        tools:layout_editor_absoluteY="152dp">

        <TextView
            android:id="@+id/textView2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="20dp"
            android:text="추천모임"
            android:textSize="23sp"
            tools:ignore="HardcodedText" />
    </LinearLayout>

    <LinearLayout
        android:id="@+id/linearLayout11"
        android:layout_width="412dp"
        android:layout_height="142dp"
        android:orientation="horizontal"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.382">

        <ImageView
            android:id="@+id/firstRandomMoim"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_margin="3dp"
            android:layout_weight="1"
            android:contentDescription="random_gruop"
            android:scaleType="centerCrop"
            app:layout_constraintEnd_toEndOf="parent"
            tools:srcCompat="@drawable/puppy"
            tools:ignore="HardcodedText" />

        <ImageView
            android:id="@+id/secondRandomMoim"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_margin="3dp"
            android:layout_weight="1"
            android:contentDescription="random_group"
            android:scaleType="centerCrop"
            app:layout_constraintEnd_toEndOf="parent"
            tools:srcCompat="@drawable/mountain"
            tools:ignore="HardcodedText" />
    </LinearLayout>

    <LinearLayout
        android:layout_width="409dp"
        android:layout_height="73dp"
        android:gravity="center_vertical"
        android:orientation="horizontal"
        tools:layout_editor_absoluteX="1dp"
        tools:layout_editor_absoluteY="368dp">

        <TextView
            android:id="@+id/textView5"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="20dp"
            android:text="내 모임"
            android:textSize="23sp"
            tools:ignore="HardcodedText" />

        <View
            android:layout_width="0dp"
            android:layout_height="0dp"
            android:layout_weight="1" />

        <Button
            android:id="@+id/button"
            android:layout_marginEnd="10dp"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="내 모임 더보기"
            tools:ignore="HardcodedText" />
    </LinearLayout>

    <TableLayout
        android:layout_width="409dp"
        android:layout_height="142dp"
        tools:layout_editor_absoluteX="1dp"
        tools:layout_editor_absoluteY="442dp">
        <TableRow
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            tools:ignore="UselessParent">

            <ImageView
                android:id="@+id/firstMyMoim"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_margin="3dp"
                android:layout_weight="1"
                android:scaleType="centerCrop"
                tools:srcCompat="@drawable/game"
                tools:ignore="ContentDescription" />

            <ImageView
                android:id="@+id/secondMyMoim"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_margin="3dp"
                android:layout_weight="1"
                android:scaleType="centerCrop"
                app:srcCompat="@drawable/universe"
                tools:ignore="ContentDescription" />

        </TableRow>
    </TableLayout>

</LinearLayout>

</ScrollView>

</androidx.constraintlayout.widget.ConstraintLayout>