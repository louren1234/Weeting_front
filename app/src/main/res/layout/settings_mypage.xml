<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity">

    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        tools:layout_editor_absoluteX="1dp"
        tools:layout_editor_absoluteY="152dp">

        <LinearLayout
            android:id="@+id/linearLayout2"
            android:layout_width="408dp"
            android:layout_height="115dp"
            android:gravity="bottom">

            <TextView
                android:id="@+id/mainpage"
                android:layout_width="270dp"
                android:layout_height="match_parent"
                android:layout_marginStart="20dp"
                android:gravity="center_vertical"
                android:text="안쓰는페이지"
                android:textColor="@color/mainColor"
                android:textSize="40sp"
                app:autoSizeMaxTextSize="40sp"
                app:autoSizeTextType="uniform" />

            <View
                android:layout_width="0dp"
                android:layout_height="0dp"
                android:layout_weight="1" />

            <ImageButton
                android:id="@+id/search"
                android:layout_width="31dp"
                android:layout_height="match_parent"
                android:layout_margin="20dp"
                android:background="@android:color/transparent"
                android:contentDescription="settings"
                android:gravity="center_vertical"
                android:scaleType="centerInside"
                app:srcCompat="@drawable/search"
                tools:ignore="HardcodedText" />

        </LinearLayout>

        <LinearLayout

            android:id="@+id/linearLayout3"
            android:layout_width="414dp"
            android:layout_height="34dp"
            android:gravity="center"
            android:orientation="horizontal">

            <ImageButton
                android:id="@+id/toHome"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:layout_weight="1"
                android:background="@android:color/transparent"
                android:contentDescription="home"
                android:scaleType="centerInside"
                app:srcCompat="@drawable/home"
                tools:ignore="HardcodedText" />

            <ImageButton
                android:id="@+id/toList"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:background="@android:color/transparent"
                android:contentDescription="list"
                android:scaleType="centerInside"
                app:srcCompat="@drawable/list"
                tools:ignore="HardcodedText" />

            <ImageButton
                android:id="@+id/toMap"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:background="@android:color/transparent"
                android:contentDescription="map"
                android:scaleType="centerInside"
                app:srcCompat="@drawable/pin"
                tools:ignore="HardcodedText" />

            <ImageButton
                android:id="@+id/toCalender"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:background="@android:color/transparent"
                android:contentDescription="calender"
                android:scaleType="centerInside"
                app:srcCompat="@drawable/calendar"
                tools:ignore="HardcodedText" />

            <ImageButton
                android:id="@+id/toMypage"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:background="@android:color/transparent"
                android:contentDescription="mypage"
                android:scaleType="centerInside"
                app:srcCompat="@drawable/user"
                tools:ignore="HardcodedText" />

        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical"
                android:gravity="center"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintTop_toTopOf="parent">

                <ImageView
                    android:layout_width="100dp"
                    android:layout_height="100dp"
                    android:layout_marginTop="30dp"
                    android:layout_marginBottom="10dp"
                    android:scaleType="centerCrop"
                    app:srcCompat="@drawable/circle"
                    tools:ignore="ContentDescription" />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="30dp"
                    android:textSize="20sp"
                    android:text="프로필 사진 변경하기"
                    tools:ignore="HardcodedText" />

            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:orientation="vertical"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent">

                <LinearLayout
                    android:id="@+id/myinform"
                    android:layout_width="410dp"
                    android:layout_height="50dp"
                    android:background="@drawable/line_updown"
                    android:layout_marginBottom="10dp"
                    android:gravity="center_vertical"
                    android:orientation="horizontal"
                    app:layout_constraintStart_toStartOf="parent"
                    tools:layout_editor_absoluteY="74dp">

                    <TextView
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="15dp"
                        android:layout_weight="1"
                        android:text="정보수정"
                        android:textSize="20sp"
                        tools:ignore="HardcodedText" />

                    <ImageView
                        android:layout_width="30dp"
                        android:layout_height="30dp"
                        android:layout_marginEnd="15dp"
                        android:adjustViewBounds="true"
                        android:background="@android:color/transparent"
                        app:srcCompat="@drawable/in"
                        tools:ignore="ContentDescription" />
                </LinearLayout>

                <LinearLayout
                    android:id="@+id/interest"
                    android:layout_width="410dp"
                    android:layout_height="50dp"
                    android:background="@drawable/line_updown"
                    android:layout_marginBottom="10dp"
                    android:gravity="center_vertical"
                    android:orientation="horizontal"
                    app:layout_constraintStart_toStartOf="parent"
                    tools:layout_editor_absoluteY="74dp">

                    <TextView
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="15dp"
                        android:layout_weight="1"
                        android:text="관심사"
                        android:textSize="20sp"
                        tools:ignore="HardcodedText" />

                    <ImageView
                        android:layout_width="30dp"
                        android:layout_height="30dp"
                        android:layout_marginEnd="15dp"
                        android:adjustViewBounds="true"
                        android:background="@android:color/transparent"
                        app:srcCompat="@drawable/in"
                        tools:ignore="ContentDescription" />
                </LinearLayout>

                <LinearLayout
                    android:id="@+id/alertSetting"
                    android:layout_width="410dp"
                    android:layout_height="50dp"
                    android:background="@drawable/line_updown"
                    android:layout_marginBottom="10dp"
                    android:gravity="center_vertical"
                    android:orientation="horizontal"
                    app:layout_constraintStart_toStartOf="parent"
                    tools:layout_editor_absoluteY="74dp">

                    <TextView
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="15dp"
                        android:layout_weight="1"
                        android:text="알림 설정"
                        android:textSize="20sp"
                        tools:ignore="HardcodedText" />

                    <ImageView
                        android:layout_width="30dp"
                        android:layout_height="30dp"
                        android:layout_marginEnd="15dp"
                        android:adjustViewBounds="true"
                        android:background="@android:color/transparent"
                        app:srcCompat="@drawable/in"
                        tools:ignore="ContentDescription" />
                </LinearLayout>

                <LinearLayout
                    android:id="@+id/faq"
                    android:layout_width="410dp"
                    android:layout_height="50dp"
                    android:background="@drawable/line_updown"
                    android:layout_marginBottom="10dp"
                    android:gravity="center_vertical"
                    android:orientation="horizontal"
                    app:layout_constraintStart_toStartOf="parent"
                    tools:layout_editor_absoluteY="74dp">

                    <TextView
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="15dp"
                        android:layout_weight="1"
                        android:text="자주 묻는 질문"
                        android:textSize="20sp"
                        tools:ignore="HardcodedText" />

                    <ImageView
                        android:layout_width="30dp"
                        android:layout_height="30dp"
                        android:layout_marginEnd="15dp"
                        android:adjustViewBounds="true"
                        android:background="@android:color/transparent"
                        app:srcCompat="@drawable/in"
                        tools:ignore="ContentDescription" />
                </LinearLayout>

                <LinearLayout
                    android:id="@+id/qna"
                    android:layout_width="410dp"
                    android:layout_height="50dp"
                    android:background="@drawable/line_updown"
                    android:layout_marginBottom="10dp"
                    android:gravity="center_vertical"
                    android:orientation="horizontal"
                    app:layout_constraintStart_toStartOf="parent"
                    tools:layout_editor_absoluteY="74dp">

                    <TextView
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="15dp"
                        android:layout_weight="1"
                        android:text="고객센터 / 문의하기"
                        android:textSize="20sp"
                        tools:ignore="HardcodedText" />

                    <ImageView
                        android:layout_width="30dp"
                        android:layout_height="30dp"
                        android:layout_marginEnd="15dp"
                        android:adjustViewBounds="true"
                        android:background="@android:color/transparent"
                        app:srcCompat="@drawable/in"
                        tools:ignore="ContentDescription,HardcodedText" />
                </LinearLayout>

                <LinearLayout
                    android:id="@+id/terms_of_use"
                    android:layout_width="410dp"
                    android:layout_height="50dp"
                    android:background="@drawable/line_updown"
                    android:layout_marginBottom="10dp"
                    android:gravity="center_vertical"
                    android:orientation="horizontal"
                    app:layout_constraintStart_toStartOf="parent"
                    tools:layout_editor_absoluteY="74dp">

                    <TextView
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="15dp"
                        android:layout_weight="1"
                        android:text="이용약관 및 개인정보 취급 방침"
                        android:textSize="20sp"
                        tools:ignore="HardcodedText" />

                    <ImageView
                        android:layout_width="30dp"
                        android:layout_height="30dp"
                        android:layout_marginEnd="15dp"
                        android:adjustViewBounds="true"
                        android:background="@android:color/transparent"
                        app:srcCompat="@drawable/in"
                        tools:ignore="ContentDescription" />
                </LinearLayout>

            </LinearLayout>

        </LinearLayout>

    </LinearLayout>

    </ScrollView>

</androidx.constraintlayout.widget.ConstraintLayout>